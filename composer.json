{
    "name": "san4aus/laravel-inline-editor",
    "description": "Laravel package, which allows inline editing of model specific content blocks.",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Niehoff",
            "email": "info@martin-niehoff.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "5.*|6.*|7.*|8.*|9.*",
        "illuminate/database": "^6.20.14|7.*|8.*|9.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "san4aus\\LaravelInlineEditor\\InlineEditorServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "san4aus\\LaravelInlineEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "san4aus\\LaravelInlineEditor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    }
}
